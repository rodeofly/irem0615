<script type="text/coffeescript">
### interpréteur de tag-systèmes (Emil Leon Post, 1921)
Licence GPL 2.0 parce que Post est rieur...
###
nr = 3 # nombre de règles
cpoisson = ['b','r','v','m','c','j','n'] # couleurs des poissons
rayon = 2 # rayon d'action de l'explosion quand un egg éclot
re = /[^rvbmcjn]/g  # une petite regex pour le nettoyage des entrées clavier
axiomelu = "bbbbbb" #écosystème, chaque egg étant représenté par une lettre donnant sa couleur
temps = 1000
allezhop = null
rules =  
	'b': 'rv',
	'r': 'b',
	'v': 'bbb',
	'm': '',
	'c': '',
	'j': '', 
	'n': ''
	
couleurs = 
	'b': 'bleu',
	'r': 'rouge',
	'v': 'vert',
	'm': 'magenta',
	'c': 'cyan',
	'j': 'jaune',
	'n': 'noir'
dessin = ->
		$("li").not(".mobile").remove()
		for lettre in axiomelu
			$("#axiome").append "<li class='egg#{couleurs[lettre]}'></li>"
		$('#axiomeclavier').val axiomelu
		for n in [1..7]
			rlue = $("#clavier#{couleurs[cpoisson[n-1]]}").val()
			rlue = rlue.replace re, ""
			for lettre in rlue
				$("#r#{couleurs[cpoisson[n-1]]}").append "<li class='egg#{couleurs[lettre]}'></li>"
			rules[cpoisson[n-1]] = rlue
			$("#clavier#{couleurs[cpoisson[n-1]]}").val rlue
#animation de la règle de production			
pondaison = ->
		if axiomelu.length >= rayon
			$("#poisson").removeClass().addClass "poisson"+couleurs[axiomelu[0]]
			$("#poisson").show 2*temps
			$("#axiome li").each (egg)->
				if egg < rayon
					$(this).hide 2*temps
			$("#poisson").animate
				left: "+="+(axiomelu.length*14-rayon*10)
				top: "-=100", (2*temps)
			axiomelu += rules[axiomelu[0]]
			axiomelu = axiomelu[rayon..]
			$("#poisson").animate
				left: "+="+(axiomelu.length*14-rayon*10)
				top: "+=100", (2*temps), ->
					dessin()
			$("#poisson").animate
				left: "+=4000", temps
			$("#poisson").hide 10
			$("#poisson").animate
				left: "0", 10


$ ->
	for n in [nr+1...8]
		$("#rang#{n}, #v#{n}").hide 100
	$("#poisson").hide 10
	$("#axiomeclavier, .hidrules").toggle 1000
	$('#axiomeclavier, #clavierbleu, #clavierrouge, #claviervert, #claviermagenta, #claviercyan, #clavierjaune, #claviernoir').on "keyup", ->
		axiomelu = $('#axiomeclavier').val()
		axiomelu = axiomelu.replace re, ""
		dessin()
	$('#crules').width(200).height(10).slider
		min: 1,
		max: 7,
		value: 3,
		slide: (evt,ui) ->
			nr = ui.value
			$(".affnr").text nr
			for n in [1..nr]
				$("#rang#{n}, #v#{n}").show 100
			for n in [nr+1...8]
				$("#rang#{n}, #v#{n}").hide 100
	$('#crayon').width(200).height(10).slider
		min: 1,
		max: 10,
		value: 2,
		slide: (evt,ui) ->
			$(".affrayon").text ui.value
	$("#clavier").on "click", ->
		$("#axiomeclavier, .hidrules").toggle 1000
	$("#flotte").on "click", ->
		pondaison()
	$("#vitesse").width(200).height(10).slider
		min: 100
		max: 2000
		value: 1000
		slide: (evt,ui) ->
			temps = ui.value
			$(".affvitesse").text (0.007*temps).toFixed(3).replace(".",",")
	$(".mobile").draggable
		revert: "valid"
	$(".regles").droppable
		drop: (evt,ui) ->
			$(this).append "<li class='egg#{couleurs[ui.draggable.text()]}'></li>"
			rules[$(this).attr('id')[1..][0]] += ui.draggable.text()
			$("#clavier#{couleurs[$(this).attr('id')[1..][0]]}").val rules[$(this).attr('id')[1..][0]]
	$("#axiome").droppable
		drop: (evt, ui) ->
			$(this).append "<li class='egg#{couleurs[ui.draggable.text()]}'></li>"
			axiomelu += ui.draggable.text()
			$('#axiomeclavier').val axiomelu
	$(".vider").on "click", ->
		rules[$(this).attr('id')[1..][0]] = rules[$(this).attr('id')[1..][0]][..-2]
		$("#clavier#{couleurs[$(this).attr('id')[1..][0]]}").val rules[$(this).attr('id')[1..][0]]
		dessin()
	$("#axiome").on "click", ->
		axiomelu = axiomelu[0..-2]
		$('#axiomeclavier').val axiomelu
		dessin()
	$("#film").on "click", ->
		if not allezhop
			pondaison()
			allezhop = window.setInterval pondaison, (7*temps)
		else
			window.clearInterval allezhop
			allezhop = null
				
                
    
</script>

<h1>Ca<strike>lcul</strike>viar</h1>
