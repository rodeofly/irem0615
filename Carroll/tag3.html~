	<tr id="rang1"><td class="poissonbleu vider" id="vbleu"></td><td><ul class='regles' id='rbleu'><li class="eggrouge"></li><li class="eggvert"></li></ul></td><td class="hidrules"><input type="text" id="clavierbleu" value="rv"></td></tr>
	<tr id="rang2"><td class="poissonrouge vider" id="vrouge"></td><td><ul class='regles' id='rrouge'><li class="eggbleu"></li></ul></td><td class="hidrules"><input type="text" id="clavierrouge" value="b"></td></tr>
	<tr id="rang3"><td class="poissonvert vider" id="vvert"></td><td><ul class='regles' id='rvert'><li class="eggbleu"></li><li class="eggbleu"></li><li class="eggbleu"></li></ul></td><td class="hidrules"><input type="text" id="claviervert" value="bbb"></td></tr>
	<tr id="rang4"><td class="poissonmagenta vider" id="vmagenta"></td><td><ul class='regles' id='rmagenta'></ul></td><td class="hidrules"><input type="text" id="claviermagenta" value=""></td></tr>
	<tr id="rang5"><td class="poissoncyan vider" id="vcyan"></td><td><ul class='regles' id='rcyan'></ul></td><td class="hidrules"><input type="text" id="claviercyan" value=""></td></tr>
	<tr id="rang6"><td class="poissonjaune vider" id="vjaune"></td><td><ul class='regles' id='rjaune'></ul></td><td class="hidrules"><input type="text" id="clavierjaune" value=""></td></tr>
	<tr id="rang7"><td class="poissonnoir vider" id="vnoir"></td><td><ul class='regles' id='rnoir'></ul></td><td class="hidrules"><input type="text" id="claviernoir" value=""></td></tr>
</table>


